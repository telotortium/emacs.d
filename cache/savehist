(defun my-org-auto-exclude-function (tag)
  \"Automatically exclude less interesting tags\"
  ;; Tags passed in are always lower-case
  (when (or
         ;; Non-work items
         (string= tag \"home\")
         ;; SICP
         (string= tag \"sicp\")
         ;; Items on hold
         (string= tag \"hold\"))
    (concat \"-\" tag)))
(setq org-agenda-auto-exclude-function 'my-org-auto-exclude-function)
